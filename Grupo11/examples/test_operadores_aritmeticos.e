@ Descripción: Ejemplo de operadores aritméticos

dato:= +4;
escribir(dato);
dato:= -4;
escribir(dato);
dato:= 3 + 4;
escribir(dato);
dato:= 3 - 4;
escribir(dato);
dato:= 3 * 4;
escribir(dato);
dato:= 3 / 4;
escribir(dato);
dato:= 3 ** 4;
escribir(dato);

@ Descripción: Ejemplo del uso de los operadores extra

++dato;
escribir(dato);
--dato;
escribir(dato);
dato+:= 3;
escribir(dato);
dato-:= 3;
escribir(dato);